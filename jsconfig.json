{
    "compilerOptions": {
        "baseUrl": "src",
        "module": "esnext",
        "esModuleInterop": true,
        "target": "es5",
        "skipLibCheck": true,
        "checkJs": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "moduleResolution": "node",
        "strict": true,
        "noImplicitAny": false,
        "paths": {
            "models/*": [
                "database/models/*"
            ],
            "db-config": [
                "database/config/config.js"
            ],
            "db": [
                "db.js"
            ]
        }
    }
}